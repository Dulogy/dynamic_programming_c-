// fibanacci no. in o(n) complexity 
#include<iostream>
using namespace std;
int fib_help(int *ar,int n){
    if(n <= 1)
    return 1;
    if(ar[n] != -1){
        return ar[n] ;
    }
    int a = fib_help(ar,n-1) ;
    int b = fib_help(ar,n-2) ;
    return a + b ;
}
int fib(int n){
    int * ar = new int[n+1] ;
    for(int i = 0; i <= n; i++){
        ar[i] = -1 ;
    }
    return fib_help(ar,n) ;

}
int main(){
    int num ;
    cin >> num ;
    int ans = fib(num) ;
    cout << "nth fib no. is " << ans ;
}
